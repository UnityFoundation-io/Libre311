    "@sveltejs/kit": "^1.30.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "svelte": "^3.54.0",
    "svelte-inview": "^4.0.0",
    "vite": "^4.3.9"
  },
  "type": "module",
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "axios": "^1.6.0",
    "flatpickr": "^4.6.13",
    "fontfaceobserver": "^2.3.0",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^2.4.0",
